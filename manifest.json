{
    "manifest_version": 3,
    "name": "Products copy from Checkbox.live",
    "description": "Products copy from Checkbox.live, with products categories.",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html",
			"default_icon": {
			"16": "icons/icon16.png",
			"32": "icons/icon32.png",
			"48": "icons/icon48.png",
			"128": "icons/icon128.png"
			}
    },
    "permissions": [
        "activeTab"
    ]
}
